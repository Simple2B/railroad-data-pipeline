    "version": "0.2.0",
    "configurations": [


        {
            "name": "scrap",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "wsgi:app",
                "FLASK_ENV": "development"
            },
            "args": [
                "scrap",
            ],
            "jinja": true
        },
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "wsgi:app",
                "FLASK_ENV": "development"
            },
            "args": [
                "run",
            ],
            "jinja": true
        },
